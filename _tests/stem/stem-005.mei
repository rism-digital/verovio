<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="https://music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
	<meiHead>
		<fileDesc>
			<titleStmt>
				<title>Stem shortening with double stem writing</title>
			</titleStmt>
			<pubStmt/>
		</fileDesc>
		<encodingDesc>
			<appInfo>
				<application version="unknown">
					<name>Verovio</name>
				</application>
			</appInfo>
		</encodingDesc>
	</meiHead>
	<music>
		<body>
			<mdiv>
				<score>
					<scoreDef>
						<staffGrp>
							<staffDef n="1" lines="5" clef.shape="G" clef.line="2"/>
							<staffDef n="2" lines="5" clef.shape="G" clef.line="2"/>
						</staffGrp>
					</scoreDef>
					<section>
						<measure n="1">
							<staff n="1">
								<layer n="1">
									<note oct="3" pname="b" dur="4" stem.dir="up"/>
									<note oct="4" pname="c" dur="4" stem.dir="up"/>
									<note oct="4" pname="d" dur="4" stem.dir="up"/>
									<note oct="4" pname="e" dur="4" stem.dir="up"/>
									<note oct="4" pname="f" dur="4" stem.dir="up"/>
									<note oct="4" pname="g" dur="4" stem.dir="up"/>
									<note oct="4" pname="a" dur="4" stem.dir="up"/>
									<note oct="4" pname="b" dur="4" stem.dir="up"/>
									<note oct="5" pname="c" dur="4" stem.dir="up"/>
									<note oct="5" pname="d" dur="4" stem.dir="up"/>
									<note oct="5" pname="e" dur="4" stem.dir="up"/>
									<note oct="5" pname="f" dur="4" stem.dir="up"/>
									<note oct="5" pname="g" dur="4" stem.dir="up"/>
									<note oct="5" pname="a" dur="4" stem.dir="up"/>
									<note oct="5" pname="b" dur="4" stem.dir="up"/>
								</layer>
							</staff>
							<staff n="2">
								<layer n="1">
									<note oct="5" pname="b" dur="4" stem.dir="down"/>
									<note oct="5" pname="a" dur="4" stem.dir="down"/>
									<note oct="5" pname="g" dur="4" stem.dir="down"/>
									<note oct="5" pname="f" dur="4" stem.dir="down"/>
									<note oct="5" pname="e" dur="4" stem.dir="down"/>
									<note oct="5" pname="d" dur="4" stem.dir="down"/>
									<note oct="5" pname="c" dur="4" stem.dir="down"/>
									<note oct="4" pname="b" dur="4" stem.dir="down"/>
									<note oct="4" pname="a" dur="4" stem.dir="down"/>
									<note oct="4" pname="g" dur="4" stem.dir="down"/>
									<note oct="4" pname="f" dur="4" stem.dir="down"/>
									<note oct="4" pname="e" dur="4" stem.dir="down"/>
									<note oct="4" pname="d" dur="4" stem.dir="down"/>
									<note oct="4" pname="c" dur="4" stem.dir="down"/>
									<note oct="3" pname="b" dur="4" stem.dir="down"/>
								</layer>
							</staff>
						</measure>
					</section>
				</score>
			</mdiv>
		</body>
	</music>
</mei>
