	<music>
		<body>
			<mdiv>
				<score>
					<scoreDef midi.bpm="400">
						<staffGrp>
							<staffDef clef.shape="G" clef.line="2" n="1" lines="5">
								<label/>
							</staffDef>
						</staffGrp>
					</scoreDef>
					<section>
						<pb/>
						<measure n="0">
							<staff n="1">
								<layer n="1">
									<note dur="1" oct="4" pname="c" accid.ges="n"/>
								</layer>
							</staff>
						</measure>
						<measure n="2">
							<staff n="1">
								<layer n="1">
									<note dur="1" oct="4" pname="d" accid.ges="n"/>
