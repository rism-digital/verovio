							</staff>
							<dir place="above" staff="1" tstamp="5.000000">
								<rend halign="right" fontstyle="normal">some text</rend>
							</dir>
						</measure>
						<sb type="original"/>
						<measure n="4">
							<staff n="1">
								<layer n="1">
									<note dur="1" oct="4" pname="c" accid.ges="n"/>
								</layer>
							</staff>
