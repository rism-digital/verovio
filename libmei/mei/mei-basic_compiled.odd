        </datatype>
      </attDef>
      <attDef ident="right" usage="opt">
        <desc xml:lang="en">Indicates the function of the right bar line and is structurally important.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.BARRENDITION"/>
        </datatype>
      </attDef>
    </attList>
