        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.PGFUNC"/>
        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.horizontalAlign" module="MEI.shared" type="atts">
    <desc xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes that record horizontal alignment.</desc>
    <attList xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="halign" usage="opt">
