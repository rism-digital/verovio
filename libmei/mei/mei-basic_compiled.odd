        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec ident="att.curvature" module="MEI.shared" type="atts" mode="replace">
                    <desc>Attributes that describe curvature.</desc>
                    <attList>
                        <attDef ident="curvedir" usage="opt">
