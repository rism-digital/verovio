  </classSpec><classSpec rend="add" ident="att.fermata.vis" module="MEI.visual" type="atts"><desc xml:lang="en">Visual domain attributes.</desc><classes><memberOf key="att.color"/><memberOf key="att.extSym"/><memberOf key="att.placementRelStaff"/><memberOf key="att.visualOffset"/></classes><attList><attDef xmlns:xi="http://www.w3.org/2001/XInclude" ident="form" usage="opt">
