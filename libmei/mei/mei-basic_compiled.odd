      <memberOf key="att.duration.additive"/>
      <memberOf key="att.startEndId"/>
      <memberOf key="att.timestamp2.log"/>
    </classes>
  </classSpec><classSpec rend="add" ident="att.enclosingChars" module="MEI.shared" type="atts">
    <desc xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes that capture characters used to enclose symbols having a cautionary or
      editorial function.</desc>
    <attList xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="enclose" usage="opt">
        <desc xml:lang="en">Records the characters often used to mark accidentals, articulations, and sometimes
          notes as having a cautionary or editorial function. For an example of cautionary
          accidentals enclosed in parentheses, see Read, p. 131, ex. 9-14.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.ENCLOSURE"/>
        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.ending.log" module="MEI.shared" type="atts">
    <desc xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Logical domain attributes.</desc>
    <classes xmlns:xi="http://www.w3.org/2001/XInclude">
      
    </classes>
  </classSpec><classSpec rend="add" ident="att.event" module="MEI.shared" type="atts"><desc>Attributes that apply to all written events, e.g., note, chord, rest, etc.</desc><classes><memberOf key="att.layerIdent" mode="add"/><memberOf key="att.staffIdent" mode="add"/></classes></classSpec><classSpec rend="add" ident="att.extender" module="MEI.shared" type="atts">
    <desc xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes that describe extension symbols, typically lines. Members of this class are
      also typically members of the att.lineRend class.</desc>
    <classes xmlns:xi="http://www.w3.org/2001/XInclude">
