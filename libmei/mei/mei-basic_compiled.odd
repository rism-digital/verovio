  </classSpec><classSpec rend="add" ident="att.octave.log" module="MEI.cmn" type="atts">
    <desc xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Logical domain attributes.</desc>
    <classes xmlns:xi="http://www.w3.org/2001/XInclude">
