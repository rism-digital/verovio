  </classSpec><classSpec rend="add" ident="att.ending.log" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Logical domain attributes.</desc>
    <classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      
    </classes>
  </classSpec><classSpec rend="add" ident="att.event" module="MEI.shared" type="atts"><desc>Attributes that apply to all written events, e.g., note, chord, rest, etc.</desc><classes><memberOf key="att.layerIdent" mode="add"/><memberOf key="att.staffIdent" mode="add"/></classes><attList/></classSpec><classSpec rend="add" ident="att.extent" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Provides attributes for describing the size of an entity.</desc>
    <classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <memberOf key="att.measurement"/>
      <memberOf key="att.ranging"/>
    </classes>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="extent" usage="opt">
        <desc xml:lang="en">Captures a measurement, count, or description. When extent contains a numeric value,
          use the unit attribute to indicate the measurement unit.</desc>
        <datatype>
          <rng:data type="string"/>
        </datatype>
        <constraintSpec ident="check_extent" scheme="isoschematron">
          <constraint>
            <sch:rule context="@extent[matches(normalize-space(.), '^\d+(\.\d+)?$')]">
              <sch:assert role="warning" test="../@unit">The @unit attribute is
                recommended.</sch:assert>
            </sch:rule>
            <sch:rule context="@extent[matches(., '\d+(\.\d+)?\s')]">
              <sch:assert role="warning" test="../@unit">Separation into value (@extent) and unit
                (@unit) is recommended.</sch:assert>
            </sch:rule>
          </constraint>
        </constraintSpec>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.grpSym.log" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Logical domain attributes.</desc>
    <classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
