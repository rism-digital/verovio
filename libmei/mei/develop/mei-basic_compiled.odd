    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="coll" usage="opt">
        <desc xml:lang="en">Indicates whether the octave displacement should be performed simultaneously with the
          written notes, <abbr>i.e.</abbr>, "coll' ottava". Unlike other octave signs which are indicated by
          broken lines, coll' ottava typically uses an unbroken line or a series of longer broken
          lines, ending with a short vertical stroke. See Read, p. 47-48.</desc>
        <valList type="closed">
          <valItem ident="coll">
            <desc xml:lang="en">Coll' ottava (with the octave).</desc>
          </valItem>
        </valList>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.pedal.log" module="MEI.cmn" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Logical domain attributes.</desc>
    <classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
