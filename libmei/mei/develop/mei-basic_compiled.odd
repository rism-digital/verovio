      <memberOf key="att.plist"/>
      <memberOf key="att.staffIdent"/>
      
      <memberOf key="att.timestamp.log"/>
      
    </classes>
  </classSpec><classSpec rend="add" ident="att.cue" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes that describe "cue-ness".</desc>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
