  </classSpec><classSpec rend="add" ident="att.accidental" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes for capturing momentary pitch inflection.</desc>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="accid" usage="opt">
