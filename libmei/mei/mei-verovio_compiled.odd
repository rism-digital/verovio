          </valItem>
          <valItem ident="daCapo">
            <desc xml:lang="en">Da capo (SMuFL E046 or Unicode 1D10A).</desc>
          </valItem>
          <valItem ident="fine">
            <desc xml:lang="en">Fine. (text)</desc>
          </valItem>
        </valList>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.responsibility" module="MEI.shared" type="atts">
