        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.PITCHNAME"/>
        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.pitched" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes that record written pitch name and octave number.</desc>
    <classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <memberOf key="att.pitch"/>
      <memberOf key="att.octave"/>
    </classes>
  </classSpec><classSpec rend="add" ident="att.placementOnStaff" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes capturing placement on a staff.</desc>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
       <attDef ident="onstaff" usage="opt">
          <desc xml:lang="en">Indicates the placement of the item within the staff. A value of '<val>true</val>' means on the staff, and '<val>false</val>' off the staff.</desc>
          <datatype>
             <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.BOOLEAN"/>
          </datatype>
       </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.placementRelEvent" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes capturing placement information with respect to an event.</desc>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
       <attDef ident="place" usage="opt">
