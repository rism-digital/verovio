        </datatype>
      </attDef>
      <attDef ident="reh.dist" usage="opt">
        <desc xml:lang="en">Records the default distance from the staff for rehearsal marks.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.MEASUREMENTSIGNED"/>
        </datatype>
      </attDef>
      <attDef ident="tempo.dist" usage="opt">
        <desc xml:lang="en">Records the default distance from the staff for tempo marks.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.MEASUREMENTSIGNED"/>
        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.dot.log" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Logical domain attributes.</desc>
    <classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
