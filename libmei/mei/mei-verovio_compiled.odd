      <memberOf key="att.pitched"/>
    </classes>
  </classSpec><classSpec rend="add" ident="att.keyMode" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes for describing key mode.</desc>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="mode" usage="opt">
