          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.CLEFLINE"/>
        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.lineRend" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes that record the visual rendition of lines.</desc>
    <classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <memberOf key="att.lineRend.base"/>
    </classes>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      
      <attDef ident="lendsym" usage="opt">
        <gloss versionDate="2022-11-11" xml:lang="en">line end symbol</gloss>
        <desc xml:lang="en">Symbol rendered at end of line.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.LINESTARTENDSYMBOL"/>
        </datatype>
      </attDef>
      <attDef ident="lendsym.size" usage="opt">
        <desc xml:lang="en">Holds the relative size of the line-end symbol.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.FONTSIZESCALE"/>
        </datatype>
      </attDef>
      <attDef ident="lstartsym" usage="opt">
        <gloss versionDate="2022-11-11" xml:lang="en">line start symbol</gloss>
