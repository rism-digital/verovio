        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.FONTNAME"/>
        </datatype>
      </attDef>
      <attDef ident="lyric.size" usage="opt">
        <desc xml:lang="en">Sets the default font size value for lyrics.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.FONTSIZE"/>
        </datatype>
      </attDef>
      <attDef ident="lyric.style" usage="opt">
        <desc xml:lang="en">Sets the default font style value for lyrics.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.FONTSTYLE"/>
        </datatype>
      </attDef>
      <attDef ident="lyric.weight" usage="opt">
