          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.PGFUNC"/>
        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.grpSym.log" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Logical domain attributes.</desc>
    <classes xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <memberOf key="att.staffGroupingSym"/>
      <memberOf key="att.startEndId"/>
    </classes>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="level" usage="opt">
        <desc xml:lang="en">Indicates the nesting level of staff grouping symbols.</desc>
        <datatype>
          <rng:data type="positiveInteger"/>
        </datatype>
      </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.handIdent" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes which identify a document hand.</desc>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="hand" usage="opt">
        <desc xml:lang="en">Signifies the hand responsible for an action. The value must be the ID of a <gi scheme="MEI">hand</gi> element declared in the header.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.URI"/>
        </datatype>
        <constraintSpec ident="check_handTarget" scheme="schematron">
          <constraint>
            <sch:rule context="@hand">
              <sch:assert role="warning" test="not(normalize-space(.) eq '')">@hand attribute should
