          <datatype>
             <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.STAFFREL"/>
          </datatype>
       </attDef>
    </attList>
  </classSpec><classSpec rend="add" ident="att.placementRelStaff" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes capturing placement information with respect to the staff.</desc>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="place" usage="opt">
