  </classSpec><classSpec rend="add" ident="att.measureNumbers" module="MEI.shared" type="atts">
    <desc xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" xml:lang="en">Attributes pertaining to measure numbers</desc>
    <attList xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
      <attDef ident="mnum.visible" usage="opt">
