        <desc xml:lang="en">Pen stroke has an extension; specific to Hispanic notation.</desc>
        <datatype>
          <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.BOOLEAN"/>
        </datatype>
      </attDef>
