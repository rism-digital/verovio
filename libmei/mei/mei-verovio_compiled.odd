        </valItem>
        <valItem ident="left">
          <desc xml:lang="en">Rotated 270 degrees clockwise.</desc>
        </valItem>
        <valItem ident="ne">
          <desc xml:lang="en">Rotated 45 degrees clockwise.</desc>
        </valItem>
        <valItem ident="nw">
          <desc xml:lang="en">Rotated 315 degrees clockwise.</desc>
        </valItem>
        <valItem ident="se">
