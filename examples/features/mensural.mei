                                <note pname="d" dur="longa" oct="4" />
                                <note pname="a" dur="longa" oct="3" />
                                <note pname="b" dur="brevis" oct="3" />
                                <note pname="d" dur="longa" oct="4" />
                                <note pname="c" dur="brevis" oct="4" />
                                <note pname="d" dur="longa" oct="4" />
                                <note pname="f" dur="longa" oct="4" />
                                <note pname="e" dur="longa" oct="4" />
                                <note pname="d" dur="longa" oct="4" />
                                <note pname="d" dur="longa" oct="4" />
                                <note pname="g" dur="longa" oct="4" />
                                <note pname="g" dur="longa" oct="4" />
                                <note pname="f" dur="longa" oct="4" />
