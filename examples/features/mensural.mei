                                <note pname="d" dur="semibrevis" oct="4"/>
                                <note pname="g" dur="minima" oct="3" num="2" numbase="3"/>
                                <!-- augm -->
                                <dot/>
                                <note pname="a" dur="semiminima" oct="3"/>
                                <note pname="b" dur="minima" oct="3"/>
                                <note pname="c" dur="minima" oct="4"/>
                                <note pname="a" dur="semibrevis" oct="3"/>
                                <barLine/>
                                <mensur tempus="2" prolatio="2" num="3" /><!-- tempus perfectum in the source -->
                                <note pname="g" dur="semibrevis" oct="3" num="2" numbase="3"/><!-- augm -->
                                <dot/>
                                <note pname="a" dur="minima" oct="3" />
                                <note pname="b" dur="semibrevis" oct="3" />
                                <note pname="c" dur="brevis" oct="4" num="2" numbase="3"/><!-- augm -->
                                <dot/>
                                <barLine/>
                                <note pname="b" dur="semibrevis" oct="3" />
                                <note pname="f" dur="semibrevis" oct="3" />
                                <note pname="b" dur="brevis" oct="3" />
                                <note pname="c" dur="brevis" oct="4" />
                                <barLine/>
                                <mensur tempus="2" prolatio="2" num="3" numbase="2" />
                                <note pname="b" dur="semibrevis" oct="3"/>
                                <rest dur="minima"/>
                                <note pname="d" dur="minima" oct="4"/>
                                <note pname="c" dur="minima" oct="4"/>
                                <note pname="e" dur="minima" oct="4" num="2" numbase="3" />
                                <!-- augm -->
                                <dot/>
                                <note pname="f" dur="semiminima" oct="4"/>
                                <note pname="g" dur="minima" oct="4"/>
                                <note pname="f" dur="minima" oct="4"/>
                                <note pname="d" dur="minima" oct="4"/>
                                <note pname="e" dur="minima" oct="4"/>
                                <note pname="e" dur="minima" oct="4"/>
                                <note pname="d" dur="minima" oct="4"/>
                                <note pname="f" dur="minima" oct="4"/>
                                <note pname="g" dur="minima" oct="4"/>
                                <note pname="d" dur="semibrevis" oct="4"/>
                                <note pname="c" dur="minima" oct="4"/>
                                <barLine/>
                                <mensur slash="1" tempus="2" prolatio="2" num="2" />
                                <note pname="d" dur="minima" oct="4" num="2" numbase="3"/>
                                <dot/>
                                <note pname="c" dur="semiminima" oct="4" />
                                <note pname="b" dur="minima" oct="3" />
                                <note pname="a" dur="minima" oct="3" />
                                <note pname="g" dur="minima" oct="3" />
                                <note pname="a" dur="minima" oct="3" />
                                <note pname="b" dur="minima" oct="3" />
                                <note pname="a" dur="minima" oct="3" />
                                <barLine/>
                                <note pname="d" dur="minima" oct="4" num="2" numbase="3"/>
                                <dot/>
                                <note pname="c" dur="semiminima" oct="4" />
                                <note pname="b" dur="minima" oct="3" />
                                <note pname="a" dur="minima" oct="3" />
                                <note pname="g" dur="brevis" oct="3" />
                                <barLine/>
                                <note pname="d" dur="semibrevis" oct="4" />
                                <note pname="g" dur="semibrevis" oct="4" />
                                <note pname="f" dur="minima" oct="4" />
                                <note pname="e" dur="semiminima" oct="4" />
                                <note pname="d" dur="semiminima" oct="4" />
                                <note pname="e" dur="semibrevis" oct="4" />
                                <barLine/>
                                <note pname="d" dur="brevis" oct="4" />
                                <rest dur="semibrevis"/>
                                <note pname="c" dur="semibrevis" oct="4" />
                                <barLine/>
                                <mensur slash="1" tempus="2" prolatio="2" num="3" />
                                <note pname="f" dur="semibrevis" oct="4" num="2" numbase="3"/>
                                <!-- augmentationis -->
                                <dot/>
                                <note pname="e" dur="minima" oct="4" />
                                <note pname="d" dur="minima" oct="4" />
                                <note pname="c" dur="minima" oct="4" />
                                <note pname="b" dur="semibrevis" oct="3" num="2" numbase="3"/>
                                <!-- augmentationis -->
                                <dot/>
                                <note pname="a" dur="minima" oct="3" />
                                <note pname="g" dur="semibrevis" oct="3" />
                                <note pname="a" dur="semibrevis" oct="3" />
                                <note pname="b" dur="semibrevis" oct="3" />
                                <note pname="a" dur="semibrevis" oct="3" />
                                <note pname="g" dur="semibrevis" oct="3" num="2" numbase="3"/>
                                <!-- augmentationis -->
                                <dot/>
                                <note pname="a" dur="minima" oct="3" />
