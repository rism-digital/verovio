                                <note pname="d" dur="longa" oct="4" colored="true" num="3" numbase="2"/>
                                <note pname="e" dur="longa" oct="4" colored="true" num="3" numbase="2"/>
                                <note pname="f" dur="longa" oct="4" colored="true" num="3" numbase="2"/>
                                <note pname="f" dur="brevis" oct="4" colored="true" num="3" numbase="2"/>
                                <note pname="e" dur="brevis" oct="4" colored="true" num="3" numbase="2"/>
                                <note pname="d" dur="longa" oct="4" colored="true" num="3" numbase="2"/>
                                <note pname="d" dur="longa" oct="4" num="1" numbase="8"/>
                                <barLine/>
                            </layer>
                        </staff>
                        <staff n="3">
                            <layer>
                                <note pname="g" dur="semiminima" oct="3" num="2" numbase="3"/>
                                <!-- augm -->
                                <dot/>
                                <note pname="a" dur="fusa" oct="3"/>
                                <note pname="b" dur="semiminima" oct="3"/>
                                <note pname="c" dur="semiminima" oct="4" num="2" numbase="3"/>
